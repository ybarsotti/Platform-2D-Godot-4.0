; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Platform 2D Godot 4.0"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=640
window/size/viewport_height=320
window/stretch/mode="viewport"
window/stretch/scale=2.0
window/canvas_textures/default_texture_filter=1

[layer_names]

2d_render/layer_1="player"
2d_render/layer_2="world"
2d_render/layer_3="enemies"
2d_render/layer_4="items"
2d_render/layer_5="hitbox"
2d_render/layer_6="hurtbox"

[rendering]

textures/canvas_textures/default_texture_filter=0
textures/vram_compression/import_etc2_astc=true
